ce":"i.js","original":{"line":56,"column":21},"name":"click"},{"generated":{"line":50,"column":22},"source":"i.js","original":{"line":56,"column":26}},{"generated":{"line":50,"column":23},"source":"i.js","original":{"line":56,"column":27}},{"generated":{"line":50,"column":24},"source":"i.js","original":{"line":56,"column":28}},{"generated":{"line":51,"column":2},"source":"i.js","original":{"line":57,"column":4}},{"generated":{"line":52,"column":0},"source":"i.js","original":{"line":58,"column":0}},{"generated":{"line":52,"column":1},"source":"i.js","original":{"line":58,"column":1}},{"generated":{"line":52,"column":2},"source":"i.js","original":{"line":58,"column":2}}],"sources":{"i.js":"import Handlebars from \"handlebars\";\n\n// Определяем шаблон как строку прямо в JavaScript\nconst commentTemplate = `\n <div class=\"cmntSection\">\n<div class=\"cmnt\">\n  <div class=\"cmntInfo\">\n    <img src=\"{{imgLink}}\" class=\"cmntImg\" alt=\"\" />\n    <h4 class=\"cmntOwner\">{{owner}}</h4>\n  </div>\n  <p class=\"cmntText\">{{text}}</p>\n</div>\n</div>\n`;\n\nconst compiledTemplate = Handlebars.compile(commentTemplate);\n\nconst commentSection = document.querySelector(\".cmntSection\");\nconst commentInput = document.querySelector(\".cmntEnterInp\");\nconst nameInput = document.querySelector(\".cmntNameEnterInp\");\nconst imageInput = document.querySelector(\".cmntImgEnter\");\nconst submitButton = document.querySelector(\".cmntEnterBtn\");\n\nfunction addComment(text, name, imgLink) {\n    const newComment = {\n        imgLink: imgLink || \"./loh.jpeg\",\n        owner: name || \"Аноним\",\n        text: text || \"Я тварь которая пишет пустой коментарий!!\"\n    };\n\n    const renderedComment = compiledTemplate(newComment);\n    commentSection.insertAdjacentHTML(\"beforeend\", renderedComment);\n}\n\nsubmitButton.addEventListener(\"click\", () => {\n    const commentText = commentInput.value.trim();\n    const userName = nameInput.value.trim();\n    const imageLink = imageInput.value.trim();\n\n    if (commentText) {\n        addComment(commentText, userName, imageLink);\n        commentInput.value = \"\";\n        nameInput.value = \"\";\n        imageInput.value = \"\";\n    }\n});\n\ncommentInput.addEventListener(\"keypress\", (event) => {\n    if (event.key === \"Enter\") {\n        submitButton.click();\n    }\n});\n\nnameInput.addEventListener(\"keypress\", (event) => {\n    if (event.key === \"Enter\") {\n        submitButton.click();\n    }\n});"},"lineCount":null}},"error":null,"hash":"7538c813ea018572a4149a3fd266a2a5","cacheData":{"env":{}}}